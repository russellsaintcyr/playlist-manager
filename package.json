{
  "name": "playlist-manager",
  "version": "17",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host 0.0.0.0",
    "start": "node server.js --port ${PORT}",
    "test": "ng test",
    "test:vitest": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:run": "vitest run",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "build:gh-pages": "ng build --configuration production",
    "deploy": "npm run build:gh-pages && npx angular-cli-ghpages --dir=dist",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "format": "prettier --write \"src/**/*.{ts,html,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,json}\""
  },
  "engines": {
    "node": "^22.0.0",
    "npm": "^10.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.15",
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/router": "^19.2.15",
    "@microsoft/clarity": "^1.0.0",
    "express": "^4.18.2",
    "firebase": "^12.6.0",
    "ngx-toastr": "^17.0.2",
    "rxjs": "^7.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.15",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/ui": "^4.0.13",
    "angular-cli-ghpages": "^2.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^8.54.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.1.0",
    "typescript": "~5.5.4",
    "vitest": "^4.0.13"
  }
}
