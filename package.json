{
  "name": "playlist-manager",
  "version": "17",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "devstart": "ng serve --host 0.0.0.0",
    "start": "node server.js --port ${PORT}",
    "test": "ng test",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "build:gh-pages": "ng build --configuration production",
    "deploy": "npm run build:gh-pages && npx angular-cli-ghpages --dir=dist",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "format": "prettier --write \"src/**/*.{ts,html,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,json}\""
  },
  "engines": {
    "node": "22.0.0",
    "npm": "10.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "express": "^4.18.2",
    "ngx-toastr": "^17.0.2",
    "rxjs": "^7.8.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "angular-cli-ghpages": "^2.0.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "~5.5.4"
  }
}
